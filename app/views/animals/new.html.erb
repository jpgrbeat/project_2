<h1>Make a new animal!</h1>

<%=form_for @animal do |f|%>
<%= f.label :name%>
<%= f.text_field :name%>
<%= f.label :type%>
<%= f.collection_select :type_of_animal_id, TypeOfAnimal.all, :id, :name%>
<%= f.label :description%>
<%= f.text_area :description%>
<%= f.label :shelter%>
<%= f.collection_select :shelter_id, Shelter.all, :id, :name%>
<%=f.submit%>
<%end%>
